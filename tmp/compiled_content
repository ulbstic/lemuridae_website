     events_by_month = events.group_by{ |item| Date.new item[:date].year, item[:date].month, 1 }.
                              sort{ |a,b| a.first <=> b.first }
     first_of_month = Date.new DateTime.now.year, DateTime.now.month, 1
     events_by_month.find_all { |i| i.first >= first_of_month }.
                     each do |month, items| %>
  <h2><%= month.strftime '%B %Y' %></h2>
    <% items.sort{ |a,b| a[:date] <=> b[:date] }.
             each do |item| %>
      <%= render "event_summary", :event => item %>
    <% end %>
  <% end %>
  <h2>Past events</h2>
  <p>
    Starting from 2011 we met up and collaborated with many wonderful people.
    <br>
    Here are several institutions and events where we shared our passion for metadata:
    <a href="http://sceti.library.upenn.edu">UPenn Schoenberg Centre for Electronic Text and Image</a>,
    <a href="http://aau.edu.et/sis/">School of Information Sciences</a> in Addis Ababa,
  <%= events.find_all { |i| i[:date] < first_of_month }.
             sort { |a,b| b[:date] <=> a[:date] }.
             map { |e| "#{link_to e[:title], e[:website]} in #{e[:location].gsub(/ /, '¬†')}" }.
             join(', ').
             sub(/, ([^,]+)$/, ' and¬†\1')
  %>.</p>
